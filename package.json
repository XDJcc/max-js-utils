{
  "name": "max-js-utils",
  "version": "1.0.2",
  "description": "max-js-utils 是一个包含常用工具函数的 JavaScript 库，提供了浅克隆、深克隆、随机数生成等功能。",
  "author": "max (xdjccemail@163.com)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "build": "babel src -d dist",
    "prepublishOnly": "npm run build",
    "readme": "npm run build && echo 'Documentation generated!'"
  },
  "keywords": [
    "mUtils",
    "utils",
    "JavaScript utilities",
    "shallCopy",
    "deepClone",
    "randomString",
    "randomRange",
    "randomNum",
    "js-utils"
