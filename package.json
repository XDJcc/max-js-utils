  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/XDJcc/max-js-utils.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XDJcc/max-js-utils.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "husky": "^7.0.4",
    "jest": "^29.7.0"
  }
}
